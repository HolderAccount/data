for i,v in next, game.Lighting:GetChildren() do
    v:Destroy()
end

for i,v in next, game:GetService("Workspace").Ignored:GetChildren() do
if v.Name == "HouseOwn" then
v:Destroy()
end
end

game:GetService("Workspace").Ignored.Shop:Destroy()
game:GetService("Workspace").Ignored.HouseItemSale:Destroy()
game:GetService("Workspace").MAP:Destroy()
game:GetService("Workspace").Lights:Destroy()
game:GetService("Workspace").Cashiers:Destroy()

for i,v in pairs (game:GetService("Workspace").Ignored:GetChildren()) do
    if v:IsA("Model") then
    v:Destroy()
end
end

task.spawn(function()
    
    repeat Wait()
        
        if game.Players.LocalPlayer.Character:FindFirstChild("FloatyPart") then
        game.Players.LocalPlayer.Character:FindFirstChild("FloatyPart"):Destroy()
        end
        
    local part = Instance.new("Part")
    part.Size = Vector3.new(3,1,3)
    part.Name = "FloatyPart"
    part.Anchored = true
    part.Parent = game.Players.LocalPlayer.Character
    part.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-3.5,0)
    part.CanCollide = true
    part.BrickColor = BrickColor.new(0,10,255)

        until false
end)
